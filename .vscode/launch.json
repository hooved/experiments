  "version": "0.2.0",
  "configurations": [

    {
      "name": "current",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      //"env": {
          //"PYTHONPATH": "${workspaceFolder}",
