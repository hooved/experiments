   "source": [
    "import py3Dmol\n",
    "from rdkit import Chem\n",
    "view = py3Dmol.view(width=600, height=400)\n",
    "\n",
    "mol = Chem.MolFromSmiles('CC(=O)Oc1ccccc1C(=O)O')  # aspirin\n",
    "mb = Chem.MolToMolBlock(mol)\n",
    "view.addModel(mb, 'sdf')\n",
    "\n",
    "view.setStyle({'stick': {}})\n",
    "view.zoomTo()\n",
    "\n",
    "view.show()"
   ]
